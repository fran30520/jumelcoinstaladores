<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activitys.activity.activitis.ResultActivity">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/main10_guide1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/main10_guide2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.9" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/main10_guide3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.9" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/main10_button_volver"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/main10_guide3"
        app:layout_constraintEnd_toStartOf="@id/main10_guide2"
        app:layout_constraintStart_toEndOf="@id/main10_guide1"
        app:cornerRadius="15dp"
        app:strokeColor="@color/color_secondary_variant"
        app:strokeWidth="2dp"
        android:layout_marginBottom="15dp"
        android:layout_marginTop="50dp"
        android:text="@string/main10_button_volver"
        />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/main10_button_confirm"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="15dp"
        android:text="@string/main9_confirm"
        app:cornerRadius="15dp"
        app:layout_constraintBottom_toTopOf="@id/main10_button_volver"
        app:layout_constraintEnd_toStartOf="@id/main10_guide2"
        app:layout_constraintStart_toEndOf="@id/main10_guide1"
        app:strokeColor="@color/color_secondary_variant"
        app:strokeWidth="2dp" />


    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/main10_text_date"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toTopOf="@id/main10_button_confirm"
        app:layout_constraintEnd_toStartOf="@id/main10_guide2"
        app:layout_constraintStart_toEndOf="@id/main10_guide1">

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/main10_textv_date"
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/main10_text_eleccion"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginBottom="20dp"
        android:text="@string/main9_txt_cita"
        app:layout_constraintBottom_toTopOf="@id/main10_text_date"
        app:layout_constraintEnd_toStartOf="@id/main10_guide2"
        app:layout_constraintStart_toEndOf="@id/main10_guide1"

        />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/main10_text_date2"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toTopOf="@id/main10_text_eleccion"
        app:layout_constraintEnd_toStartOf="@id/main10_guide2"
        app:layout_constraintStart_toEndOf="@id/main10_guide1">

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/main10_textv_date2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            />
    </com.google.android.material.textfield.TextInputLayout>


    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/main10_textv_date3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/main9_hi"
        app:layout_constraintBottom_toTopOf="@id/main10_text_date2"
        app:layout_constraintEnd_toStartOf="@id/main10_guide2"
        app:layout_constraintStart_toEndOf="@id/main10_guide1"

        />

    <ImageView
        android:id="@+id/main10_jumelcoLogo"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:gravity="center"
        android:padding="20dp"
        android:src="@mipmap/ic_jumelco_logo_final"
        app:layout_constraintBottom_toTopOf="@id/main10_textv_date3"
        app:layout_constraintEnd_toStartOf="@id/main10_guide2"
        app:layout_constraintStart_toEndOf="@id/main10_guide1"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="@color/color_secondary_variant"
        app:strokeWidth="2dp" />


</androidx.constraintlayout.widget.ConstraintLayout>